{
  "name": "expo-app",
  "description": "Bulletproof Expo SDK 49 Template",
  "version": "1.0.0",
  "author": {
    "name": "Tri Rizeki Rifandani",
    "email": "tri.rifandani@gmail.com"
  },
  "private": false,
  "main": "expo-router/entry",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write src/",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "i18n": "typesafe-i18n",
    "config:introspect": "expo config --type introspect",
    "install:fix": "expo install --fix",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "dev": "expo start",
    "dev:clean": "expo start -c",
    "start": "run-p i18n dev",
    "start:clean": "run-p i18n dev:clean",
    "start:prod": "run-p i18n dev:prod",
    "build:android": "eas build --profile development --platform android",
    "build:android:local": "eas build --profile development --platform android --local",
    "build:ios": "eas build --profile development-simulator --platform ios",
    "build:ios:local": "eas build --profile development-simulator --platform ios --local",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "release": "bumpp"
  },
  "dependencies": {
    "@expo/metro-config": "~0.17.3",
    "@expo/vector-icons": "^14.0.0",
    "@formatjs/intl-getcanonicallocales": "^2.3.0",
    "@formatjs/intl-locale": "^3.4.5",
    "@formatjs/intl-pluralrules": "^5.2.12",
    "@hookform/resolvers": "^3.3.4",
    "@rifandani/nxact-yutiriti": "^1.2.2",
    "@tamagui/animations-moti": "^1.86.6",
    "@tamagui/babel-plugin": "^1.86.6",
    "@tamagui/config": "^1.86.6",
    "@tamagui/theme-builder": "^1.86.6",
    "@tamagui/toast": "^1.86.6",
    "@tanstack/react-query": "^4.36.1",
    "burnt": "^0.11.7",
    "expo": "^50.0.2",
    "expo-application": "~5.8.3",
    "expo-av": "~13.10.3",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.6",
    "expo-font": "~11.10.2",
    "expo-image": "~1.10.4",
    "expo-linking": "~6.2.2",
    "expo-localization": "~14.8.3",
    "expo-router": "~3.4.4",
    "expo-splash-screen": "~0.26.3",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "ky": "^1.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.3",
    "react-native": "0.73.2",
    "react-native-gesture-handler": "~2.14.1",
    "react-native-mmkv": "^2.11.0",
    "react-native-reanimated": "~3.6.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "tamagui": "^1.86.6",
    "type-fest": "^4.9.0",
    "typesafe-i18n": "^5.26.2",
    "zod": "^3.22.4",
    "zod-validation-error": "^1.5.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@types/react": "~18.2.48",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  }
}
